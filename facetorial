// factor.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;
void main()
{
	int n, I, P=1;
	
do{
	cout<<"ingrese n";
	cin>>n;
} while (n<0);
for (I=1;I<=n;I++);
	P=P*I;
cout<<"el producto es";
cin>>P;

getch();
}
